{
  "name": "cyclops",
  "version": "0.4.3",
  "description": "Cyphon frontend",
  "main": "server/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "travis": "jest --coverage --maxWorkers=4 && cat ./coverage/lcov.info | coveralls",
    "sphinx": "cd sphinx-docs && make html",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack",
    "start": "webpack-dev-server --progress",
    "lint": "tslint 'src/**/*.{ts,tsx}'"
  },
  "author": "Chase Brewer <chase.brewer@dunbarsecured.com>",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/src/app/$1",
      "\\.scss$": "<rootDir>/test/scssMock.js"
    },
    "collectCoverageFrom": [
      "src/app/**/*.{ts,tsx}"
    ]
  },
  "license": "ISC",
