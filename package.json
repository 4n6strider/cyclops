{
  "name": "cyclops",
  "version": "1.0.0",
  "description": "Cyphon frontend",
  "main": "build/express/server.js",
  "scripts": {
    "build:css": "node-sass --include-path node_modules/bootstrap-sass/assets/stylesheets src/scss/app.scss build/static/css/main.css",
    "build:express": "tsc -p src/express",
    "watch:css": "npm run build:css && chokidar 'src/scss' -c 'npm run build:css'",
    "watch:express": "tsc -p src/express -w",
    "server:dev": "NODE_ENV=DEV nodemon build/express/server.js --watch build/express",
    "server:prod": "NODE_ENV=PROD node build/express/server.js",
    "webpack:prod": "webpack --config webpack/webpack.prod.config.js",
    "webpack:dev": "webpack --config webpack/webpack.dev.config.js --watch",
    "test:react": "karma start",
    "test:express": "mocha --opts mocha.opts",
    "docs": "typedoc --out docs --target ES6 --tsconfig typedoc.json --readme DOCUMENTATION.md --includeDeclarations --ignoreCompilerErrors src/",
    "livereload": "livereload \"build/static,build/webpack\"",
    "clean": "del build",
    "build": "npm run clean && npm run build:express && npm run build:css && npm run webpack:prod",
    "dev": "npm run clean && npm run build:express && npm run build:css && concurrently -p name -c \"bgRed.bold,bgGreen.bold,bgBlue.bold,bgYellow.bold,bgMagenta.bold\" -n \"Express,CSS,Server,Webpack,Livereload\" \"npm run watch:express\" \"npm run watch:css\" \"npm run server:dev\" \"npm run webpack:dev\" \"npm run livereload\""
  },
