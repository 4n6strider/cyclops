{
  "name": "cyclops",
  "version": "2.0.1",
  "description": "Cyphon frontend",
  "main": "server/index.js",
  "scripts": {
    "server:dev": "cross-env NODE_ENV=development nodemon",
    "server:prod": "cross-env NODE_ENV=production ts-node --fast server --cache-directory ./cache",
    "client:dev": "cross-env NODE_ENV=development webpack --watch",
    "client:prod": "cross-env NODE_ENV=production webpack --progress",
    "test:watch": "cross-env NODE_ENV=test karma start",
    "test": "cross-env NODE_ENV=test karma start --single-run true",
    "travis": "npm run test && cat ./coverage/lcov.info | coveralls",
    "docs:api": "typedoc --out api-docs --target ES6 --tsconfig typedoc.json --readme DOCUMENTATION.md --includeDeclarations --ignoreCompilerErrors src/",
    "docs:sphinx": "cd sphinx-docs && make html",
    "livereload": "livereload \"dist\"",
    "clean": "del dist",
    "start": "npm run server:prod",
    "build": "npm run clean && npm run client:prod",
    "dev": "npm run clean && concurrently -p name -c \"bgRed.bold,bgGreen.bold,bgBlue.bold\" -n \"Server,Webpack,Livereload\" \"npm run server:dev\" \"npm run client:dev\" \"npm run livereload\""
  },
